{
  "name": "wq8pw",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:docker": "concurrently npm:serve npm:express",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "express": "vue-cli-service express:watch",
    "express:run": "vue-cli-service express:run",
    "test:unit": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "codecov": "codecov",
    "make:config": "cp config.default.json config.json",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "adjuster": "^1.4.0",
    "axios-digest": "^0.1.1",
    "body-parser": "^1.19.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "rfc4648": "^1.4.0",
    "simple-des-crypto": "0.0.1",
    "type-array-convert": "0.0.1",
    "typeorm": "^0.2.25",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/mongoose": "^5.7.32",
    "@types/morgan": "^1.9.1",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.10",
    "@types/winston": "^2.4.4",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/test-utils": "1.0.3",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.2",
    "codecov": "^3.7.2",
    "concurrently": "^5.2.0",
    "fibers": "^5.0.0",
    "request": "^2.88.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-express": "^1.0.2",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/views/",
      "/src/main/",
      "/srv/migration/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,js,vue}",
      "!**/*.d.ts",
      "!**/plugins/**",
      "!**/coverage/**",
      "!**/*config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
